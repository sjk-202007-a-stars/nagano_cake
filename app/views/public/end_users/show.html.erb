<div class="container">
  <div class="row">
    <div class="col-lg-offset-1">
      <h4>マイページ</h4>
      <div class="col-lg-5">
        <strong>登録情報</strong>
        <%= link_to "編集する", edit_end_user_path,class:"btn btn-primary  col-lg-offset-1" %>
        <%= link_to "パスワード変更する", edit_end_user_registration_path, class:"btn btn-primary" %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-lg-6">
      <table class="table col-lg-offset-2">
        <tr>
          <td class="active">氏名</td>
          <td><%= @end_user.family_name %><%= @end_user.first_name %></td>
        </tr>
        <tr>
          <td class="active">カナ</td>
          <td><%= @end_user.family_name_kana %><%= @end_user.first_name_kana %></td>
        </tr>
        <tr>
          <td class="active">郵便番号</td>
          <td><%= @end_user.postal_code %></td>
        </tr>
        <tr>
          <td class="active">住所</td>
          <td><%= @end_user.address %></td>
        </tr>
        <tr>
          <td class="active">電話番号</td>
          <td><%= @end_user.phone_number %></td>
        </tr>
        <tr>
            <td class="active">メールアドレス</td>
            <td><%= @end_user.email %></td>
        </tr>
      </table>
    </div>
  </div>
  <div class="row">
    <div class="col-lg-5 col-lg-offset-1 shipping_addresses">
      <strong>配送先</strong>
      <%= link_to "一覧を見る", shipping_addresses_path, class:"btn btn-primary col-lg-offset-1" %>
    </div>
  </div>
  <div class="row">
    <div class="col-lg-offset-1 col-lg-5 orders">
      <strong>注文一覧</strong>
      <%= link_to "一覧を見る", orders_path, class:"btn btn-primary col-lg-offset-1" %>
    </div>
  </div>
</div>