<div class="container">
    <div class="row">
        <div class="col-xs-12">
            <h2>
                <span class="bg-success">会員一覧</span>
            </h2>
        </div>
    </div>
</div>
<div class="col-xs-7">
	<table class="table">
	    <thead class="thead-dark">
	      <tr class="active">
	        <th>会員ID</th>
	        <th>氏名</th>
	        <th>メールアドレス</th>
	        <th>ステータス</th>
	      </tr>
	    </thead>
	    <tbody>
			<% @end_users.each do |end_user| %>
				<tr>
					<td><%= end_user.id %></td>
			 		 <td>
			 		 	<%= link_to admin_end_user_path(end_user) do %>
			 		 		<%= end_user.family_name %><%= end_user.first_name %>
			 		 	<% end %>
			 		 </td>
			  		<td><%= end_user.email %></td>
			  		<td>
			  			<% if end_user.is_deleted %>
			  				退会済み
			  			<% else %>
			  				有効
			  			<% end %>
			        </td>
			    </tr>
			<% end %>
	    </tbody>
	</table>
</div>